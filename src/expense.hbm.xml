<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="houserholdaccountbook.Expense" table="EXPENSE">
		<id name="expenseCode" type="int" unsaved-value="null">
			<column name="EXPENSE_CODE" sql-type="int" />
			<generator class="assigned" />
		</id>
		<property name="name" type="string" column="NAME" />
		<property name="displayOrder" type="string" column="DISPLAY_ORDER" />
	</class>
</hibernate-mapping>